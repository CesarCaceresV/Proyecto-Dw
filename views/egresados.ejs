<div class="panel panel-default panel-primary" style="margin-top:15px;">
  	<div class="panel-heading">Lista de egresados</div>
  	<div class="panel-body">
  		<button class="btn btn-success" data-toggle="modal" data-target="#myModal">Agregar nuevo Usuario</button>	
  		<table class="table table-striped">
      		<thead>
        		<tr>
          			<th>Nombre</th>
		          	<th>Apellido</th>
		          	<th>Email</th>
		          	<th>Opciones</th>
		        </tr>
      		</thead>
      		<tbody>
				<tr>
					<td>Cristian</td>
					<td>Rojas</td>
					<td>cristian.rojas@alumnos.uv.cl</td>
					<td>
						<button class="btn btn-primary">Editar</button>
						<button class="btn btn-danger">Eliminar</button>
					</td>
				</tr>
				<tr>
					<td>Ivette</td>
					<td>León</td>
					<td>ivette.leon@alumnos.uv.cl</td>
					<td>
						<button class="btn btn-primary">Editar</button>
						<button class="btn btn-danger">Eliminar</button>
					</td>
				</tr>
			</tbody>
    	</table>
  	</div>
</div>

<div id="myModal" class="modal fade" role="dialog">
  	<div class="modal-dialog modal-lg">
  	<!-- Modal content-->
    	<div class="modal-content">
      		<div class="modal-header" style="background-color:blue;color:white">
        		<button type="button" class="close" data-dismiss="modal">&times;</button>
        		<h4 class="modal-title">Agregar nuevo usuario</h4>
      		</div>
      		<div class="modal-body">
        		<form method="POST" id="formulario">
        			<div class="container-fluid">
					    <div class="col-lg-4">
					 
					 
					        <div class="form-group">
			        			<label for="nombre">Nombre</label>
			        			<input type="text" class="form-control" name="nombre" id="nombre">
			        		</div>	 
					 
					 
					    </div>
					    <div class="col-lg-4">
					 
					 
					        <div class="form-group">	
					        	<label for="apellido">Apellido</label>
					        	<input type="text" class="form-control" name="apellido" id="apellido">
			        		</div>
					 
					         
					    </div>
					    <div class="col-lg-4">
					 
					        <div class="form-group">
					        	<label for="rut">Rut</label>
					        	<input type="text" class="form-control" name="rut" id="rut">
				        	</div>
					         
					    </div>
					</div>
					<div class="container-fluid">
					    <div class="col-lg-4">
					 
					 
					        <div class="form-group">
					        	<label for="ingreso">Año Ingreso</label>
					        	<input type="date" class="form-control" name="ingreso" id="ingreso">
				        	</div>					 
					 
					    </div>
					    <div class="col-lg-4">
					 
					 
					        <div class="form-group">
					        	<label for="egreso">Año Egreso</label>
					        	<input type="date" class="form-control" name="egreso" id="egreso">
				        	</div>
					 
					         
					    </div>
					    <div class="col-lg-4">
					 
					        <div class="form-group">
					        	<label for="titulacion">Año Titulación</label>
					        	<input type="date" class="form-control" name="titulacion" id="titulacion">
				        	</div>
					         
					    </div>
					</div>
					<div class="container-fluid">
					    <div class="col-lg-4">
					 
					        <div class="form-group">
					        	<label for="carrera">Carrera</label>
					        	<select class="form-control" name="carrera" id="carrera">
					        		<option>Ejecución</option>
					        		<option>Informática</option>
					        		<option>Civil</option>
					        	</select>
				        	</div>					 
					 
					    </div>
					    <div class="col-lg-4">
					 
					        <div class="form-group">
					        	<label for="postgrado">Postgrado</label>
					        	<select class="form-control" name="postgrado" id="postgrado">
					        		<option>Magister</option>
					        		<option>Doctorado</option>
					        	</select>
					        </div>
					         
					    </div>
					    <div class="col-lg-4">
					 
					        <div class="form-group">
					      		<label for="a_postgrado">Área Postgrado</label>
					        	<select class="form-control" name="a_postgrado" id="a_postgrado">
					        		<option>Informática</option>
					        		<option>Negocio</option>
					        		<option>Otros</option>
					        	</select>
					      	</div>
					         
					    </div>
					</div>
					<div class="container-fluid">
					    <div class="col-lg-4">
					 
					        <div class="form-group">
					        	<label for="satisfaccion">Satisfacción Carrera</label>
					        	<input type="text" class="form-control" name="satisfaccion" id="satisfaccion">
					        </div>					 
					 
					    </div>
					    <div class="col-lg-4">
					 
					        <div class="form-group">
					        
					        </div>
					         
					    </div>
					    <div class="col-lg-4">
					 
					        <div class="form-group">
					      		<label for="cv">Curriculum Vitae</label>
					        	<div class="input-group">
					                <label class="input-group-btn">
					                    <span class="btn btn-default">
					                        Explorar...<input type="file" style="display: none;" id="cv" name="cv">
					                    </span>
					                </label>
					                <input type="text" class="form-control" readonly>
					            </div>
					      	</div>
					         
					    </div>
					</div>
					<div class="container-fluid">
					    <div class="col-lg-12">
					 
					        <div class="form-group">
					        	<label for="linkedin">Linkedin</label>
					        	<input type="url" class="form-control" name="linkedin" id="linkedin">
					        </div>					 
					 
					    </div>
					</div>
					<div class="container-fluid">
					    <div class="col-lg-12">
					 
					        <div class="form-group">
					        	<label for="capacitacion">Necesidad Capacitación</label>
					        	<textarea name="capacitacion" id="capacitacion" class="form-control"></textarea>
					        </div>					 
					 
					    </div>
					</div>
        		</form>
      		</div>
      		<div class="modal-footer">
        		<input class="btn btn-success" type="submit" value="Guardar" form="formulario">
      		</div>
    	</div>
  	</div>
</div>